local function convertCountry()
	local countryPlayer = countryToNationality[countryCode]
	if countryPlayer then
		return countryPlayer
	else
		local var = "???"
		return var
	end
end

local country = convertCountry()

local function getAccountInfo()
    local accountAge = LocalPlayer.AccountAge
    local creationTime = os.time() - (accountAge * 86400)
    local dateTable = os.date("*t", creationTime)

    local monthNames = {
        "Enero", "Febrero", "Marzo", "Abril", "Mayo", "Junio",
        "Julio", "Agosto", "Septiembre", "Octubre", "Noviembre", "Diciembre"
    }

    local dayNames = {
        "Domingo", "Lunes", "Martes", "MiÃ©rcoles", 
        "Jueves", "Viernes", "SÃ¡bado"
    }

    return {
        day = dateTable.day,
        month = monthNames[dateTable.month],
        year = dateTable.year,
        weekday = dayNames[dateTable.wday],
        accountAge = accountAge,
        fullDate = string.format("%02d/%02d/%d", dateTable.day, dateTable.month, dateTable.year),
        prettyDate = string.format("%d de %s de %d", dateTable.day, monthNames[dateTable.month], dateTable.year)
    }
end

local info = getAccountInfo()

    local placeId = game.PlaceId
