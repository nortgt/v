PremiumTab:Section({Title = Translator:text("Vehicle Speed Unlocker"), Icon = "sparkles"})

local speedValue = 200
local turboValue = 20

-- Function to find all cars in the workspace
local function getAllCars()
    local cars = {}
    if workspace.Vehicles then
        for _, vehicle in pairs(workspace.Vehicles:GetChildren()) do
            if vehicle:FindFirstChild("Seats") and vehicle.Seats:FindFirstChild("VehicleSeat") then
                table.insert(cars, vehicle)
            end
        end
    end
    return cars
end

-- Function to apply values â€‹â€‹to all cars
local function applyToAllCars(valueType, value)
    local cars = getAllCars()
    local carsModified = 0
    
    if #cars == 0 then
        newNotify("âŒ Failed", "No cars found in Workspace.Vehicles")
        return
    end
    
    for _, vehicle in pairs(cars) do
        local vehicleSeat = vehicle.Seats.VehicleSeat
        newNotify("âŒ› Wait", "Checking car: " .. vehicle.Name)
        
        -- List all children of VehicleSeat for debug
        print("âš ï¸ Objects in VehicleSeat:")
        for _, child in pairs(vehicleSeat:GetChildren()) do
            print("âš ï¸ " .. child.Name .. " (" .. child.ClassName .. ")")
        end
        
        local targetObj = vehicleSeat:FindFirstChild(valueType)
        
        if targetObj then
            print("âš ï¸ Found " .. valueType .. " (" .. targetObj.ClassName .. ")")
            
            if targetObj:IsA("NumberValue") then
                targetObj.Value = value
                carsModified = carsModified + 1
                print("âš ï¸ " .. valueType .. " (NumberValue) changed to " .. value)
            elseif targetObj:IsA("IntValue") then
                targetObj.Value = value
                carsModified = carsModified + 1
                print("âš ï¸ " .. valueType .. " (IntValue) changed to " .. value)
            elseif targetObj:IsA("StringValue") then
                targetObj.Value = tostring(value)
                carsModified = carsModified + 1
                print("âš ï¸ " .. valueType .. " (StringValue) changed to " .. value)
            else
                print("âš ï¸ " .. valueType .. " is the type " .. targetObj.ClassName)
                -- Try changing directly
                pcall(function()
                    targetObj.Value = value
                    carsModified = carsModified + 1
                    print("âš ï¸ " .. valueType .. " directly changed to " .. value)
                end)
            end
        else
            print("âŒ Failed " .. valueType .. " not found in VehicleSeat")
            
            -- Try as direct property of VehicleSeat
            local success = pcall(function()
                if valueType == "MaxSpeed" then
                    vehicleSeat.MaxSpeed = value
                elseif valueType == "Turbo" then
                    vehicleSeat.Turbo = value
                end
                carsModified = carsModified + 1
                print("âš ï¸ " .. valueType .. " (VehicleSeat property) changed to " .. value)
            end)
            
            if not success then
                print("âŒ Failed to change " .. valueType .. " as property")
            end
        end
    end
    newNotify("âœ… Success", "Total number of modified cars: " .. carsModified)
end

PremiumTab:Input({
    Title = Translator:text("Speed Value"),
    Desc = "Enter a value to apply to the vehicle",
    Callback = function(Value)
        local numValue = tonumber(Value)
        if numValue then
            speedValue = numValue
            print("âœ… Applied speed value: " .. speedValue)
        else
            print("âš ï¸ Enter a value to apply speed")
        end
    end
})

PremiumTab:Input({
    Title = Translator:text("Turbo Value"),
    Desc = "Enter a value to apply to the vehicle",
    Callback = function(Value)
        local numValue = tonumber(Value)
        if numValue then
            turboValue = numValue
            print("âœ… Applied turbo value: " .. turboValue)
        else
            print("âš ï¸ Enter a value to apply turbo")
        end
    end
})

PremiumTab:Button({
    Title = Translator:text("Apply Speed"),
    Icon = "chevron-right",
    Callback = function()
        newNotify("âœ… Success", "Speed Applied " .. speedValue .. " in all cars")
        applyToAllCars("MaxSpeed", speedValue)
    end
})

PremiumTab:Button({
    Title = Translator:text("Apply Turbo"),
    Icon = "chevron-right",
    Callback = function()
        newNotify("âœ… Success", "Turbo Applied " .. turboValue .. " in all cars")
        applyToAllCars("Turbo", turboValue)
    end
})

