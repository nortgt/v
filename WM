local totalTime = 10
local steps = 100
local delayPerStep = totalTime / steps

for i = 1, steps do
    ProgressBar.Size = UDim2.new(i/steps, 0, 0, 6)
    PercentText.Text = i .. "%"
    wait(delayPerStep)
end

PercentText.Text = "100%"
Desc.Text = "Done!"
task.wait(0.5)

TweenService:Create(Background, TweenInfo.new(0.6), {BackgroundTransparency = 1}):Play()
