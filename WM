AvatarTab:Section({Title = Translator:text("Advanced Animation Customizer"), Icon = "film"})

local idleAnim = {
    {Name = "adidas Sport", ID = 18538150608},
    {Name = "adidas Community", ID = 126354114956642},
    {Name = "Bold Animation", ID = 16744209868},
    {Name = "Catwalk Glam", ID = 101279640971758},
    {Name = "No Boundaries", ID = 18755930927}
}

local idleAnimName = {}
for _, idAnim in ipairs(idleAnim) do
    table.insert(idleAnimName, idAnim.Name)
end

AvatarTab:Dropdown({
    Title = Translator:text("Select Idle Animation"),
    Values = idleAnimName,
	Value = idleAnimName[1],
    Callback = function(value)
        for _, idAnim in ipairs(idleAnim) do
            if idAnim.Name == value then
                idAnimID = idAnim.ID
                break
            end
        end
    end
})

AvatarTab:Button({
	Title = Translator:text("Equip Idle Animation"),
    Icon = "chevron-right",
    Callback = function()
	    if idAnimID then
    	    Wear:InvokeServer(idAnimID)
    	end
    end
})

local walkAnim = {
    {Name = "adidas Sport", ID = 18538146480},
    {Name = "adidas Community", ID = 106810508343012},
    {Name = "Bold Animation", ID = 16744219182},
    {Name = "Catwalk Glam", ID = 103190462987721},
    {Name = "No Boundaries", ID = 18755942776}
}

