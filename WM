MusicTab:Section({Title = Translator:text("Server Audio Player"), Icon = "music"})

local audios = {
    {Name = "Amongus sound", ID = 6651571134},
    {Name = "Sus", ID = 6701126635},
    {Name = "SUS", ID = 7153419575},
    {Name = "Sonic.exe", ID = 2496367477},
