for _, cmd in ipairs(comandos) do
	AdminTab:Button({
    	Title = cmd:lower(),
        Desc = "Script for ."..cmd.." - Target",
    	Icon = "circle-small",
        Locked = false,
        Callback = function()
        	if cmd == "verify" then
            	EnviarComando("verify", "")
        	else
            	if TargetName and TargetName ~= "" then
                	EnviarComando(cmd, TargetName)
                else
                	newNotify("No player selected")
            end
        end
	end})
end

