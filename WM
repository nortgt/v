local function applyKamuiLighting()
	Lighting.Ambient = Color3.new(0,0,0)
	Lighting.OutdoorAmbient = Color3.new(0,0,0)
	Lighting.FogColor = Color3.new(0,0,0)
	Lighting.FogStart = 20
	Lighting.FogEnd = 180
end

local function restoreNormalLighting()
